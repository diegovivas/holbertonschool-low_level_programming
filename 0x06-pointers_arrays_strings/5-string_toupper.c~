#include "holberton.h"
/**
 * reverse_array - check the code for Holberton School students.
 *@a:integer
 *@n:integer
 * Return: Always 0.
 */
char *string_toupper(char *)
{
	int i = 0;
 
	while (string[i] != '\0') 
	{
		if (string[i] >= 'a' && string[i] <= 'z') {
			string[i] = string[i] - 32;
		}
		i++;
	}

}
